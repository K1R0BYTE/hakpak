alias -g cdi='cd `ls | peco`'
alias -g xcopy='xclip -selection clipboard'
alias -g xpaste='xclip -selection clipboard -o'
alias -g lsa='ls -lsah'
alias -g mstcpcls='grep tcp | cut -d " " -f 3 | sort -u  | sed -z "s/\n/,/g"'
alias -g msudpcls='grep udp | cut -d " " -f 3 | sort -u  | sed -z "s/\n/,/g"'
alias -g eth='ip -4 addr show eth0 | grep -oP "(?<=inet\s)\d+(\.\d+){3}"'
alias -g tun='ip -4 addr show tun0 | grep -oP "(?<=inet\s)\d+(\.\d+){3}"'
alias -g mkalldir='mkdir nmap; mkdir masscan; mkdir webscan; mkdir screenshots; mkdir findings'
alias -g startrdp='sudo service xrdp start'
alias -g stoprdp='sudo service xrdp stop'
alias -g startdhcp='sudo service isc-dhcp-server start'
alias -g stopdhcp='sudo service isc-dhcp-server stop'